package com.example.rules

import java.util.*

import com.example.*


rule "Rule that breaks ExecutableModel"
@filename("rules.drl")
ruleflow-group "RFG-1"
salience 5
no-loop true
    when
    	$list: List()
        $cwv1: ClassWithValue( $v: value )
        $cwv2: ClassWithValue( value == $v - 1 )
        
    then
    	$list.add($cwv2);
end
